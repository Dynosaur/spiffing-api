    "express": "^4.17.1",
    "jest": "^26.5.2",
    "jest-serial-runner": "^1.1.0",
    "mongodb": "^3.6.0",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
